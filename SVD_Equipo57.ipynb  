        "    \n",
        "    kfold = RepeatedKFold(n_splits=5, n_repeats=3)\n",
        "\n",
        "    scores = cross_validate(\n",
        "                            estimator = model,\n",
        "                            X = X_train_t,\n",
        "                            y = y_train_t.ravel(),\n",
        "                            scoring=metrics,\n",
        "                            cv = kfold,\n",
        "                            return_train_score=True\n",
        "                            )\n",
        "\n",
        "    results.append(scores)\n",
        "\n",
        "    print(model_name[i])\n",
        "    for metric in metrics.keys():\n",
        "        print(\n",
        "            '\\t%s\\n\\t\\tTrain: %.3f (%.4f)\\n\\t\\tTest: %.3f (%.4f)' % (\n",
